<% cancel_dialog = true if cancel_dialog.nil? %>

<div class="am-form-group">
	<div class="am-u-sm-9 am-u-sm-push-3">
		<%= f.submit "提交", class: "am-btn am-btn-primary"%>
		<%= link_to "返回", params[:back], class: "am-btn am-btn-primary tpl-btn-bg-color-success " if params[:back].present? %>
		<%= link_to_void "取消", onclick: "cancel_dialog()", class: "am-btn am-btn-primary tpl-btn-bg-color-success " if cancel_dialog %>
	</div>
</div>